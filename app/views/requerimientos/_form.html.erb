<%= semantic_form_for(@requerimiento) do |f| %>
  <%= f.inputs do %>
    <%= f.input :empresa, :collection => Empresa.enabled %>
    <%= f.input :sector, :collection => Sector.enabled %>
    <%= f.input :rubro, :collection => Rubro.enabled %>
  <% end %>
  <%= f.buttons do %>
    <% f.commit_button "Continuar" %>
  <% end %>
<% end %>

