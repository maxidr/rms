<h1>Listado de materiales</h1>

<table>
  <tr>
    <th>Material</th>
    <th>Cantidad</th>
    <th>Detalle</th>
  </tr>

<% @materiales.each do |material| %>
  <tr>
    <td><%= material.nombre %></td>
    <td><%= material.cantidad %></td>
    <td><%= material.detalle %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo material', new_requerimiento_material_path %>

